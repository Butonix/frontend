{
  "name": "frontend",
  "description": "Frontend side of the King of Travel project",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "lint": "eslint src/**/*",
    "test": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "svelteSortOrder": "markup-scripts-styles",
    "svelteStrictMode": true,
    "svelteBracketNewLine": true
  },
  "browserslist": [
    "last 2 versions",
    "firefox esr",
    "not dead",
    "not ie 11",
    "not ie_mob 11",
    "not op_mini all",
    "not op_mob > 0",
    "not and_qq > 0",
    "not android > 0"
  ],
  "dependencies": {
    "@editorjs/header": "^2.3.2",
    "@editorjs/list": "^1.4.0",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/table": "^1.2.1",
    "@github/auto-check-element": "^5.1.0",
    "@rollup/plugin-json": "^4.0.1",
    "compression": "^1.7.1",
    "connect-pg-simple": "^6.1.0",
    "express-session": "^1.17.0",
    "http-proxy-middleware": "^0.20.0",
    "pg": "^7.17.1",
    "polka": "next",
    "sirv": "^0.4.0",
    "svelte-flatpickr": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@editorjs/editorjs": "^2.16.1",
    "@rollup/plugin-alias": "^3.0.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^4.0.10",
    "npm-run-all": "^4.1.5",
    "postcss-custom-media": "^7.0.8",
    "postcss-import": "^12.0.1",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.0",
    "svelte": "^3.17.1",
    "svelte-infinite-scroll": "^0.1.1",
    "svelte-preprocess": "^3.3.0",
    "svelte-select": "^3.3.5",
    "svelte-tabs": "^1.1.0"
  }
}
